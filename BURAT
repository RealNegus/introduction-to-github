#include <stdio.h>
#include <stdlib.h>

int main();
void classroomPage();
void bscsPage();
void bsoaPage();
void bsoaafPage();
void educPage();

void classroomdisplay () {
	printf("\n\t\t\t=============================================================================================================================\n");
    printf("\t\t\t*                                                  Classroom Management Page                                               *\n");
    printf("\t\t\t============================================================================================================================\n");
}

int main () {
	int user;
	 printf("\n\t\t\t============================================================================================================================\n");
    printf("\t\t\t*                                                   Professor Menu Page                                                    *\n");
    printf("\t\t\t============================================================================================================================\n");
    
  
    printf("\t\t\t\t\t\t-----------------------------------------------------------\n");
    printf("\t\t\t\t\t\t|              [ 1 ] Classroom Management                 |\n");
    printf("\t\t\t\t\t\t-----------------------------------------------------------\n");
    printf("\t\t\t\t\t\t-----------------------------------------------------------\n");
    printf("\t\t\t\t\t\t|              [ 0 ] Exit                                 |\n");
    printf("\t\t\t\t\t\t-----------------------------------------------------------\n");
    
    printf("");
    scanf("%d", &user);
    
    if (user == 1) {
    	classroomPage();
	}
    return 0;
}

void classroomPage () {
	
	int user1;
	
	system("cls");
	classroomdisplay();
	printf("\t\t\t\t\t|----------------------------------------------------------|\n");
	printf("\t\t\t\t\t|                     Choose a course:                     |\n");
	printf("\t\t\t\t\t|----------------------------------------------------------|\n");
	printf("\t\t\t\t\t|                    [ 1 ] BSCS1M1                         |\n");
	printf("\t\t\t\t\t|                    [ 2 ] BSOA1M1                         |\n");
	printf("\t\t\t\t\t|                    [ 3 ] BSOA1N1                         |\n");
	printf("\t\t\t\t\t|                    [ 4 ] EDUC1M1                         |\n");
	printf("\t\t\t\t\t|----------------------------------------------------------|\n");
	
	printf("");
    scanf("%d", &user1);
    
	switch (user1) {
		case 1:
			bscsPage();
			break;
		case 2:
			bsoaPage();
			break;
		case 3:
			bsoaafPage();
			break;
		case 4:
			educPage();
			break;
		default:
			printf("Invalid");
	}
	system("pause");
}

